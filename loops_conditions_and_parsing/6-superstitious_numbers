#!/usr/bin/env bash
#task to use case
n=1;
while [ $n -le 20 ]; do
		printf "\n%s" "$n"
		(( n++ ))

case $n in
5)
	printf "\n%s"	"bad luck from China"
	;;
10)
	        printf "\n%s" "bad luck from Japan"
		        ;;
18)
			printf "\n%s" "bad luck from Italy"					                        ;;
esac
done
